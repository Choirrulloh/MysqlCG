# Maintainer: Reliefs
pkgname=MysqlCG
pkgver=0.1
pkgrel=1
pkgdesc="MysqlCG - Mysql Client Gui"
arch=("any")
url="https://github.com/reliefs/MysqlCG"
license=('MIT')
depends=('bash' 'mysql')
source=("git://github.com/reliefs/${pkgname}/")
sha1sums=('SKIP')

package() {
	    cd "$pkgname"
	        mkdir -p $pkgdir/usr/bin
		    install -D -m755 ./mysql.sh $pkgdir/usr/bin/mysqlcg
	    }
sha1sums=('SKIP')
